}
.btn_page a:hover {
	position:relative;
	background: transparent url('/Src/Img/Renewal/btn_contents_bg.gif') no-repeat  scroll left -20;
	color:#666666;
	cursor:pointer	;
	display:block;
	float:left;
	font-family:"돋움", Dotum;
 	font-size:11px;
	font-weight:normal;
	height:20px;
	line-height:normal;
	text-decoration:none;
	padding-left:6px;
	margin-right:3px;
	overflow:hidden;
}
.btn_page a span {
	position:relative;
	background: transparent url('/Src/Img/Renewal/btn_contents_bg.gif') no-repeat scroll right 0;
	display:block;
