}
.btn_page2 a {
	cursor:pointer	;
}
.btn_page2 a:hover {
	position:relative;
	background: transparent url('/Src/Img/Renewal/btn2_contents_bg.gif') no-repeat scroll left -40px;
}


.btn_page2 a span , .btn_page2 span{
