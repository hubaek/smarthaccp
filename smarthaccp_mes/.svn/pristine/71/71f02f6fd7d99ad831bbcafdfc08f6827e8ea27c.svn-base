	<subDataset name="detail1" uuid="4873c2b5-2be4-4a88-a07d-e41733211257">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="georyu"/>
		<parameter name="slipcd" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT B.SLIP_SEQ,
       B.SLIP_CD,
       C.ITEM_NM,      /* 품명 */
       C.UNIT,             /* 단위 */
	   FORMAT(TRUNCATE(B.ITEM_QTY,0),0) ITEM_QTY,     /* 수량 */
	   FORMAT(TRUNCATE(B.UNIT_AMT,0),0) UNIT_AMT,     /* 단가 */
	   FORMAT(TRUNCATE(B.SUPPLY_AMT,0),0) SUPPLY_AMT,   /* 공급가 */
	   FORMAT(TRUNCATE(B.SURTAX_AMT,0),0) SURTAX_AMT    /* 세액 */
FROM TB_TEMP_REPORT A
     LEFT OUTER JOIN TB_MES_SA310 B ON A.SLIP_SEQ = B.SLIP_SEQ AND SLIP_CD =  $P{slipcd}  AND B.SLIP_SEQ <12
	 LEFT JOIN TB_MES_ITEM000 C ON B.COMPANY = C.COMPANY AND B.ITEM_CD = C.ITEM_CD
WHERE 1=1]]>
		</queryString>
		<field name="SLIP_SEQ" class="java.lang.String"/>
		<field name="SLIP_CD" class="java.lang.String"/>
		<field name="ITEM_NM" class="java.lang.String"/>
		<field name="UNIT" class="java.lang.String"/>
		<field name="ITEM_QTY" class="java.lang.String"/>
		<field name="UNIT_AMT" class="java.lang.String"/>
		<field name="SUPPLY_AMT" class="java.lang.String"/>
		<field name="SURTAX_AMT" class="java.lang.String"/>
		<group name="group1"/>
	</subDataset>
	<parameter name="slipcd" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT /* 공급받는자 보관용 */
       A.SLIP_CD, /* 수주번호 */
	   CONCAT(CONCAT(CONCAT(SUBSTRING(A.SA_ORDER_DT,1,4),'년 '),CONCAT(SUBSTRING(A.SA_ORDER_DT,6,2),'월 ')),CONCAT(SUBSTRING(A.SA_ORDER_DT,9,2),'일')) SA_ORDER_DT,  /* 수주일자 */
       ifnull(C.BUSINESS_NO, '') CP_BUSINESS_NO, /* 등록번호 */
       ifnull(C.ACCOUNT_NO, '') ACCOUNT_NO,      /* 계좌번호 */
	   ifnull(C.COMPANY_NM, '') CP_COMPANY_NM,   /* 상호 */
       ifnull(C.CEO_NM, '') CP_CEO_NM,           /* 대표 */
       ifnull(CONCAT(C.ADDRESS1,C.ADDRESS2), '') CP_ADDRESS, /* 주소 */
	   ifnull(FORMAT(TRUNCATE((SELECT SUM(Z.SUPPLY_AMT) FROM TB_MES_SA310 Z WHERE Z.COMPANY = A.COMPANY AND  Z.SLIP_CD = A.SLIP_CD),0),0),0) SUM_SUPPLY_PRICE,/* 금액 */
	   ifnull(FORMAT(TRUNCATE((SELECT SUM(Z.SURTAX_AMT) FROM TB_MES_SA310 Z WHERE Z.COMPANY = A.COMPANY AND  Z.SLIP_CD = A.SLIP_CD),0),0),0) SUM_SUR_TAX,     /* 세액 */
       ifnull(FORMAT(TRUNCATE((SELECT SUM(Z.TOTAL_AMT) FROM TB_MES_SA310 Z WHERE Z.COMPANY = A.COMPANY AND  Z.SLIP_CD = A.SLIP_CD),0),0),0) SUM_TOTAL_PRICE,  /* 합계 */
       ifnull(CONCAT(getNumToKOR(lpad(TRUNCATE((SELECT SUM(Z.TOTAL_AMT) FROM TB_MES_SA310 Z WHERE Z.COMPANY = A.COMPANY AND  Z.SLIP_CD = A.SLIP_CD),0),16,'0')),'(',FORMAT(TRUNCATE((SELECT SUM(Z.TOTAL_AMT) FROM TB_MES_SA310 Z WHERE Z.COMPANY = A.COMPANY AND  Z.SLIP_CD = A.SLIP_CD),0),0),')'),0) KOR_AMT,
       ifnull(C.LEADER_NAME,'') as LEADER_NAME,
       ifnull(C.TEL,'') as TEL,
       ifnull(C.FAX,'') as FAX,
       /* 공급자 보관용 */
       ifnull(B.BUSINESS_NO,'') as BUSINESS_NO, /* 등록번호 */
	   ifnull(B.CUST_NM,'') as CUST_NM, /* 상호 */
       ifnull(B.CEO_NM,'') as CEO_NM,               /* 대표 */
       ifnull(B.ADDRESS,'') as ADDRESS 
       'http://localhost/image/transaction_T.png' as path
FROM TB_MES_SA300 A,
     TB_MES_CUST000 B,
     TB_MES_CP000 C
WHERE A.COMPANY = B.COMPANY
AND   A.CUST_CD = B.CUST_CD
AND   A.COMPANY = C.COMPANY
AND   A.SLIP_CD = $P{slipcd}]]>
	</queryString>
