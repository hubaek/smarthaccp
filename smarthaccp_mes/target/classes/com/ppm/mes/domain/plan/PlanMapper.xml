<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ppm.mes.domain.plan.PlanMapper">

<select id="maxYear" resultType="planVO" statementType="PREPARED">
/*com.ppm.mes.domain.plan.PlanMapper.maxPlan*/
	SELECT
		MAX(QC_YYYY) AS maxYear
	FROM
		TB_MES_QC500
</select>

</mapper>        